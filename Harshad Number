class Solution {
    public int sumOfTheDigitsOfHarshadNumber(int x) {
        int k = x;
        int t = 0;
        int sum = 0;
        while(k > 0){
            t = k % 10;
            sum += t;
            System.out.println(sum);
            k = k/10;

        }
        if(x % sum == 0){
            return sum;
        }
        else{return -1;}
    }
}
